{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Mufi",
  "logo": {
    "dark": "/logo/mufi-logo-white.svg",
    "light": "/logo/mufi-logo-white.svg"
  },
  "favicon": "/favicon.svg",
  "colors": {
    "primary": "#9ca3ff",
    "light": "#9ca3ff",
    "dark": "#080408",
    "anchors": {
      "from": "#0D9373",
      "to": "#07C983"
    },
    "background": {
      "light": "#080408",
      "dark": "#080408"
    }
  },
  "layout": "sidenav",
  "theme": "prism",
  "background": {
    "style": "gradient"
  },
  "modeToggle": {
    "default": "dark",
    "isHidden": true
  },
  "sidebar": {
    "items": "container"
  },
  "search": {
    "location": "top"
  },
  "topbarCtaButton": {
    "name": "Get Started",
    "url": "mailto:connect@mufi.app",
    "style": "roundedRectangle",
    "arrow": true
  },
  "navigation": [
    {
      "group": "",
      "pages": [
        "introduction",
        "quickstart"
      ]
    },
    {
      "group": "Wallets API",
      "pages": [
        "wallet-api/introduction",
        "wallet-api/authentication",
        "wallet-api/security"
      ]
    },
    {
      "group": "API Playground",
      "pages": [
        "wallet-api/endpoint/auth",
        "wallet-api/endpoint/refreshAuth",
        "wallet-api/endpoint/version",
        "wallet-api/endpoint/wallets"
      ]
    }
  ],
  "footerSocials": {
    "website": "https://mufi.app",
    "twitter": "https://x.com/MusicFinance",
    "discord": "https://t.me/mufi_wallet",
    "medium": "https://github.com/mintlify",
    "linkedin": "https://www.linkedin.com/company/mufi"
  }
}