{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Mufi",
  "logo": {
    "dark": "/logo/mufi-logo-white.svg",
    "light": "/logo/mufi-logo-white.svg"
  },
  "favicon": "/favicon.svg",
  "colors": {
    "primary": "#B7B1FB",
    "light": "#B7B1FB",
    "dark": "#080408",
    "anchors": {
      "from": "#0D9373",
      "to": "#07C983"
    },
    "background": {
      "light": "#080408",
      "dark": "#080408"
    }
  },
  "layout": "sidenav",
  "theme": "prism",
  "background": {
    "style": "gradient"
  },
  "modeToggle": {
    "default": "dark",
    "isHidden": true
  },
  "sidebar": {
    "items": "container"
  },
  "search": {
    "location": "top"
  },
    "topbarCtaButton": {
    "name": "Get Started",
    "url": "https://mintlify.com/start",
    "style": "roundedRectangle",
    "arrow": false
  },
  "navigation": [
    {
      "group": "Get Started",
      "pages": [
        "introduction",
        "quickstart",
        "development"
      ]
    },
    {
      "group": "Essentials",
      "pages": [
        "essentials/markdown",
        "essentials/code",
        "essentials/images",
        "essentials/settings",
        "essentials/navigation",
        "essentials/reusable-snippets"
      ]
    },
    {
      "group": "API Documentation",
      "pages": [
        "wallet-api/introduction"
      ]
    },
    {
      "group": "API Playground",
      "pages": [
        "wallet-api/endpoint/get",
        "wallet-api/endpoint/create",
        "wallet-api/endpoint/delete"
      ]
    }
  ],
  "footerSocials": {
    "website": "https://mufi.app",
    "twitter": "https://x.com/MusicFinance",
    "discord": "https://t.me/mufi_wallet",
    "medium": "https://github.com/mintlify",
    "linkedin": "https://www.linkedin.com/company/mufi",
  }
}